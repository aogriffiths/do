{
  "name": "do",
  "version": "1.0.0",
  "description": "Writing about things I do",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build-ghpages": "env NODE_ENV=production npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
    "start": "npx @11ty/eleventy --serve --quiet",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "debugstart": "DEBUG='Eleventy* nbconvert*' npx @11ty/eleventy --serve --quiet",
    "benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "install-python": "python -m venv python_venv && source python_venv/bin/activate && pip install -r requirements.txt"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aogriffiths/do.git"
  },
  "author": {
    "name": "Adam Griffiths",
    "email": "aogriffiths+do@gmail.com",
    "url": "https://github.com/aogriffiths"
  },
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/aogriffiths/do/issues"
  },
  "homepage": "https://github.com/aogriffiths/do",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@hpcc-js/wasm": "^2.8.0",
    "json-to-dot": "^1.1.0",
    "luxon": "^3.2.1",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-eleventy-img": "^0.10.0",
    "parse5": "^7.1.2",
    "render-dot": "^0.0.7",
    "yaml": "^2.2.1"
  }
}
